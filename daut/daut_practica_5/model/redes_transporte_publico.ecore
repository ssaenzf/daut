  </eAnnotations>
  <eClassifiers xsi:type="ecore:EClass" name="RedTransporte">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="nonDuplicateLineas nonDuplicateParadas"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="nonDuplicateLineas" value="&#xA;&#x9;&#x9;&#x9;linea->forAll(linea1, linea2 | &#xA;&#x9;&#x9;&#x9;&#x9;linea1 &lt;> linea2 implies ((linea1.codigo &lt;> linea2.codigo) and (linea1.oclIsTypeOf(linea2.oclType())))&#xA;&#x9;&#x9;&#x9;)"/>
      <details key="nonDuplicateParadas" value="&#xA;&#x9;&#x9;&#x9;parada->forAll(parada1, parada2 | &#xA;&#x9;&#x9;&#x9;&#x9;parada1 &lt;> parada2 implies (parada1 &lt;> parada2)&#xA;&#x9;&#x9;&#x9;)"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="linea" lowerBound="1" upperBound="-1"
        eType="#//Linea" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="parada" lowerBound="2"
        upperBound="-1" eType="#//Parada" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Metro" eSuperTypes="#//Linea">
