      <details key="constraints" value="nonDuplicateColores"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="nonDuplicateColores" value="&#xA;&#x9;&#x9;&#x9;color->forAll(color1, color2 | &#xA;&#x9;&#x9;&#x9;&#x9;color1 &lt;> color2 implies (color1 &lt;> color2)&#xA;&#x9;&#x9;&#x9;)"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="paradas: Parada"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="color" eType="#//Colores"
        defaultValueLiteral=""/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Bus" eSuperTypes="#//Linea">
